# inclui  < stdio.h >

int  main (){
    dupla nota1, nota2, notafinal;

    printf ( " Digite a primeira nota: " );
    scanf ( " %lf " , ¬a1);

	printf ( " Digite a segunda nota: " );
    scanf ( " %lf " , ¬a2);

    notafinal = nota1 + nota2;

    printf ( " NOTA FINAL = %.1lf \n " , notafinal);

    if (notafinal < 60,0 ) {
        printf ( " REPROVADO \n " );
    }

	retorna  0 ;
}

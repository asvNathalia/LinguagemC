#include <stdio.h>

int main ()
{
    int X, Y;

    printf ("Digite os dois numeros:\n");
    scanf ("%d", &X);
    scanf ("%d", &Y);

    while (X != Y) {
        if (X < Y){
            printf ("CRESCENTE\n");
        }
        else {
            printf ("DECRESCNTE\n");
        }
        printf ("Digite outros dois numeros\n");
        scanf ("%d", &X);
        scanf ("%d", &X);
    }




    return 0;
}
